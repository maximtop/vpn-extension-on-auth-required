{
    "manifest_version": 2,
    "name": "VPN Extension OnAuthRequired Sample",
    "short_name": "VPN Extension OnAuthRequired Sample",
    "author": "maximtop@gmail.com",
    "version": "0.0.1",
    "description": "VPN Extension OnAuthRequired Sample",
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "<all_urls>",
        "proxy",
        "webRequest",
        "browsingData",
        "webRequestBlocking"
    ]
}
